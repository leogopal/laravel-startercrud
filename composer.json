{
    "name": "leogopal/laravel-startercrud",
    "description": "A CRUD Artisan helper for the Laravel Starterkit",
    "keywords": [
        "laravel",
        "leogopal",
        "laravel-startercrud"
    ],
    "homepage": "https://github.com/leogopal/laravel-startercrud",
    "license": "MIT",
    "authors": [
        {
            "name": "Leo Gopal",
            "email": "leo@leogopal.com",
            "homepage": "https://leogopal.com",
            "role": "Maintainer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Leogopal\\LaravelStartercrud\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        },
        "laravel": {
            "providers": [
                "Leogopal\\LaravelStartercrud\\ServiceProvider"
            ]
        }
    }
}
